    "formats": {
      "name": "formats",
      "help": "List supported export formats for different document types.\n\nExamples:\n    gwt formats\n    gwt formats -t spreadsheet\n    gwt formats -t presentation 
--json",
      "options": [
        {
          "name": "doc_type",
          "type": "STRING",
          "required": false,
          "help": "Document type (document, spreadsheet, presentation)",
          "flags": [
            "--type",
            "-t"
          ],
          "default": "document",
          "multiple": false
        },
        {
          "name": "output_json",
          "type": "BOOL",
          "required": false,
          "help": "Output as JSON",
          "flags": [
            "--json",
            "-j"
          ],
          "default": false,
          "multiple": false
        }
      ],
      "arguments": []
    },
    "extract-id": {
      "name": "extract-id",
      "help": "Extract document ID from a Google Drive URL.\n\nExamples:\n    gwt extract-id https://docs.google.com/document/d/abc123/edit\n    gwt extract-id 
\"https://docs.google.com/spreadsheets/d/xyz789/edit#gid=0\"",
      "options": [],
      "arguments": [
        {
          "name": "url",
          "type": "STRING",
          "required": true,
          "help": "Google Drive URL to extract ID from",
          "multiple": false
        }
      ]
    },
    "mail": {
      "name": "mail",
      "help": "Export Gmail messages to JSON or Markdown.\n\nExamples:\n    gwt mail -q \"from:boss@example.com\" -f md\n\n    gwt mail -a 2024-01-01 -l 
